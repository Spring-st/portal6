<?xml version="1.0" encoding='UTF-8'?>
<!DOCTYPE hibernate-mapping PUBLIC  "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd" >
<hibernate-mapping package="com.aof.model.product">
	<class name="SalesPreshiporderBatch" table="sales_PreShipSrder_Batch">
		<id name="id" column="id" type="integer">
			<generator class="native" />
		</id>
		<many-to-one name="spsoitemId" column="SPSOITEM_ID" class="com.aof.model.product.SalesPreshiporderItem" />
		<many-to-one name="box" column="BOX" class="com.aof.model.po.Box" />
		<property name="status" column="STATUS" type="com.aof.model.metadata.YesNo" />
		<property name="enabled" column="ENABLED" type="com.aof.model.metadata.EnabledDisabled" />
	</class>
</hibernate-mapping>
