<?xml version="1.0" encoding='UTF-8'?>
<!DOCTYPE hibernate-mapping PUBLIC  "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd" >
<hibernate-mapping package="com.aof.model.product">
	<class name="WorkOrderBom" table="work_order_bom">
		<id name="id" column="id" type="integer">
			<generator class="native" />
		</id>
		<property name="workOrderId" column="work_order_id" type="string" />
		<property name="workOrderNo" column="WORK_ORDER_NO" type="string" />
		<property name="productNo" column="PRODUCT_NO" type="string" />
		<property name="partNo" column="PART_NO" type="string" />
		<property name="allNeedQty" column="ALL_NEED_QTY" type="string" />
		<property name="workingOrder" column="WORKING_ORDER" type="string" />
		<property name="workingPosition" column="WORKING_POSITION" type="string" />
		<property name="singleNeedQty" column="SINGLE_NEED_QTY" type="big_decimal" />
		<property name="site" column="SITE" type="string" />
		<property name="domain" column="DOMAIN" type="string" />
		<property name="seqNo" column="SEQ_NO" type="string" />
	</class>
</hibernate-mapping>
