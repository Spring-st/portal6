<?xml version="1.0" encoding='UTF-8'?>
<!DOCTYPE hibernate-mapping PUBLIC  "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd" >
<hibernate-mapping package="com.aof.model.product">
	<class name="CustomerPlan" table="customer_plan">
		<id name="id" column="id" type="integer">
			<generator class="native" />
		</id>
		<property name="planNumbers" column="PLAN_NUMBERS" type="string" />
		<property name="line" column="LINE" type="string" />
		<many-to-one name="customer" column="CUSTOMER" class="com.aof.model.product.BasicCustomer" />
		<many-to-one name="wmspart" column="WMSPART" class="com.aof.model.basic.WmsPart" />
		<property name="um" column="UM" type="string" />
		<property name="qty" column="QTY" type="big_decimal" />
		<property name="receiptQty" column="RECEIPT_QTY" type="big_decimal" />
		<property name="qtyOpen" column="QTY_OPEN" type="big_decimal" />
		<property name="returnQtySum" column="RETURN_QTY_SUM" type="big_decimal" />
		<property name="shipmentDate" column="SHIPMENT_DATE" type="timestamp" />
		<property name="entryDate" column="ENTRY_DATE" type="timestamp" />
		<property name="describe" column="DESCRIBE" type="string" />
		<property name="poDomain" column="PO_DOMAIN" type="string" />
		<many-to-one name="site" column="SITE" class="com.aof.model.admin.Site" />
		<property name="factory" column="FACTORY" type="string" />
		<property name="isprint" column="ISPRINT" type="com.aof.model.metadata.YesNo" />
		<property name="customerAddress" column="CUSTOMER_ADDRESS" type="string" />
		<property name="unitPrice" column="UNIT_PRICE" type="big_decimal" />
		<property name="sotaxc" column="SOTAXC" type="string" />
		<property name="operation" column="OPERATION" type="string" />
		<property name="curr" column="CURR" type="string" />
		<property name="status" column="STATUS" type="integer" />
		
	</class>
</hibernate-mapping>
