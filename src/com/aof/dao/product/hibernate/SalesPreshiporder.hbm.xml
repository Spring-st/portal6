<?xml version="1.0" encoding='UTF-8'?>
<!DOCTYPE hibernate-mapping PUBLIC  "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd" >
<hibernate-mapping package="com.aof.model.product">
	<class name="SalesPreshiporder" table="sales_preShipOrder">
		<id name="id" column="id" type="integer">
			<generator class="native" />
		</id>
		<many-to-one name="site" column="SITE" class="com.aof.model.admin.Site" />
		<many-to-one name="requestorId" column="REQUESTOR_ID" class="com.aof.model.admin.User" />
		<property name="code" column="CODE" type="string" />
		<property name="createDate" column="CREATE_DATE" type="timestamp" />
		<property name="remark" column="REMARK" type="string" />
		<property name="isPrint" column="IS_PRINT" type="com.aof.model.metadata.YesNo" />
		<property name="status" column="STATUS" type="com.aof.model.metadata.SalesPreshiporderStatus" />
		<property name="arrivaldate" column="ARRIVALDATE" type="timestamp" />
		<property name="isEnabled" column="IS_ENABLED" type="com.aof.model.metadata.EnabledDisabled" />
		<property name="customerName" column="customer_name" type="string" />
		<property name="customerCode" column="customer_code" type="string" />
		<property name="shPrint" column="ship_print" type="com.aof.model.metadata.YesNo" />
		<property name="type" column="shiporder_type" type="integer" />
		<property name="shPrintDate" column="PRINT_DATE" type="timestamp" />
		<property name="matchStatus" column="MATCH_STATUS" type="com.aof.model.metadata.YesNo" />
		
	</class>
</hibernate-mapping>
