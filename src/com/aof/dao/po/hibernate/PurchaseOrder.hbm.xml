		<property name="isPrintLabels" column="is_print_labels" type="com.aof.model.metadata.YesNo" />
		<property name="inventoryNumber" column="inventory_Number" type="big_decimal" />
		<property name="receiptQty" column="receipt_qty" type="big_decimal" />
		<property name="returnNumber" column="return_Number" type="big_decimal" />
		<property name="ReturnQtySum" column="return_qty_sum" type="big_decimal" />
