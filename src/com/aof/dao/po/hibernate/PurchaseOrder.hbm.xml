<hibernate-mapping package="com.aof.model.po">
<!-- ==============================采购业务开始2015-01-28-st================================================ -->
	<!-- 采购单主表 -->
	<class name="PurchaseOrder" table="po">
		<id name="id" column="id" type="integer">
			<generator class="assigned" />
		</id>
		<many-to-one name="site" column="site" class="com.aof.model.admin.Site" />
		<many-to-one name="department" column="department" class="com.aof.model.admin.Department"/>
		<many-to-one name="supplier" column="supplier_code" class="com.aof.model.admin.Supplier" />
		
		<property name="domain" column="domain" type="string" />
		<property name="po_date" column="po_date" type="date" />
		<property name="po_order" column="po_order" type="string" />
		<property name="describe" column="describe" type="string" />
		<property name="status" column="status" type="com.aof.model.metadata.PurchaseOrderStatus" />
		<property name="receipts_type" column="receipts_type" type="com.aof.model.metadata.PurchaseOrderReceiptsType" />
		<property name="type" column="type" type="com.aof.model.metadata.PurchaseOrderType" />
		<property name="address" column="address" type="string" />
		<property name="remark" column="remark" type="string" />
	</class>
	<!-- 采购单明细表  -->
	<class name="PurchaseOrderDetial" table="po_detial">
		<id name="id" column="id" type="integer">
			<generator class="assigned" />
		</id>
		<many-to-one name="po_id" column="po_id" class="PurchaseOrder" />
		<many-to-one name="part" column="part" class="com.aof.model.basic.WmsPart"/>
		
		<property name="line" column="line" type="integer" />
		<property name="supp_part" column="supp_part" type="string" />
		<property name="unit" column="unit" type="string" />
		<property name="due_date" column="due_date" type="date" />
		<property name="po_qty" column="po_qty" type="big_decimal" />
		<property name="received_qty" column="received_qty" type="big_decimal" />
		<property name="arrival_qty" column="arrival_qty" type="big_decimal" />
		<property name="status" column="status" type="com.aof.model.metadata.YesNo" />
		<property name="address" column="address" type="string" />
		<property name="remark" column="remark" type="string" />
		<property name="capacity" column="capacity" type="big_decimal" />
	</class>
	<!-- 采购收货主表  -->
	<class name="PurchaseOrderReceipts" table="po_receipts">
		<id name="id" column="id" type="integer">
			<generator class="native" />
		</id>
		<many-to-one name="site" column="site" class="com.aof.model.admin.Site" />
		<many-to-one name="department" column="department" class="com.aof.model.admin.Department"/>
		
		<property name="code" column="code" type="string" />
		<property name="start_date" column="start_date" type="date" />
		<property name="end_date" column="end_date" type="date" />
		<property name="status" column="status" type="com.aof.model.metadata.PurchaseOrderReceiptsStatus" />
		<property name="plan_sum_number" column="plan_sum_number" type="big_decimal" />
		<property name="actual_sum_number" column="actual_sum_number" type="big_decimal" />
		<property name="remark" column="remark" type="string" />
	</class>
	<!-- 采购收货明细表  -->
	<class name="PurchaseOrderReceiptsDetial" table="po_receipts_detial">
		<id name="id" column="id" type="integer">
			<generator class="native" />
		</id>
		<many-to-one name="po_receipts_id" column="po_receipts_id" class="PurchaseOrderReceipts" />
		<many-to-one name="po_detial_id" column="po_detial_id" class="PurchaseOrderInspectionPendingItem" />
		
		<property name="plan_number" column="plan_number" type="big_decimal" />
		<property name="actual_number" column="actual_number" type="big_decimal" />
		<property name="status" column="status" type="com.aof.model.metadata.YesNo" />
		<property name="line" column="line" type="string" />
		<property name="po_order" column="po_order" type="string" />
		<property name="remark" column="remark" type="string" />
		<property name="po_part" column="po_part" type="string" />
		<property name="date" column="date" type="date" />
		
		<property name="test1" column="test1" type="string" />
		<property name="test2" column="test2" type="string" />
		<property name="test3" column="test3" type="string" />
	</class>
	<!-- 收货单 批次关联表  -->
	<class name="PurchaseOrderReceiptsDetialBox" table="po_receipts_detial_box">
		<id name="id" column="id" type="integer">
			<generator class="native" />
		</id>
		
		<many-to-one name="box_id" column="box_id" class="Box" />
		<many-to-one name="po_receipts_detial_id" column="po_receipts_detial_id" class="PurchaseOrderInspectionPendingItem" />
	</class>
	<class name="WmsLot" table="basic_lot">
		<id name="id" column="lot_id" type="string">
			<generator class="assigned" />
		</id>
		
		<property name="enabled" column="status" type="com.aof.model.metadata.EnabledDisabled" not-null="true" />
	</class>
	<!-- 批次表  -->
	<class name="Box" table="purchaseOrder_box">
		<id name="id" column="id" type="integer">
			<generator class="native" />
		</id>
		<many-to-one name="lot" column="lot_ser" class="WmsLot" />
		<many-to-one name="location" column="location" class="com.aof.model.basic.StorageLocation" />
		<many-to-one name="part" column="part" class="com.aof.model.basic.WmsPart" />
		<many-to-one  name="psoItem" column="psoItem"  class="PortalShipOrderItem"/>
		<many-to-one  name="tool" column="tool"  class="com.aof.model.basic.WmsTool"/>
		<many-to-one  name="single" column="single"  class="PurchaseOrderCondimentSingle"/>
		
		<property name="number" column="count" type="big_decimal" />
		<property name="in_date" column="in_date" type="date" />
		<property name="out_date" column="out_date" type="date" />
		<property name="date" column="date" type="date" />
		<property name="print_number" column="print_number" type="integer" />
		<property name="status" column="status" type="com.aof.model.metadata.BoxStatus" />
		<property name="status_rqc" column="status_rqc" type="com.aof.model.metadata.BoxStatusRqc" />
		<property name="status_print" column="status_print" type="com.aof.model.metadata.YesNo" />
		<property name="status_freeze" column="status_freeze" type="com.aof.model.metadata.YesNo" />
		<property name="supplierCode" column="supplier" type="string" />
		<property name="type" column="type" type="com.aof.model.metadata.YesNo" />
		<property name="enabled" column="enabled" type="com.aof.model.metadata.EnabledDisabled" />
		
		<property name="isReceipt" column="isReceipt" type="com.aof.model.metadata.YesNo" />
		<property name="isPrint" column="is_print" type="com.aof.model.metadata.YesNo" />
		<property name="receivedNumber" column="receivedNumber" type="big_decimal" />
		<property name="vetoReceivedNumber" column="vetoReceivedNumber" type="big_decimal" />
		<property name="inStorageNumber" column="inStorageNumber" type="big_decimal" />
		<property name="vetoQCnumber" column="vetoQCnumber" type="big_decimal" />
		<property name="returnsNumber" column="returnsNumber" type="big_decimal" />
		<property name="supplierBatch" column="supplierBatch" type="string" /> 
		<property name="isSync" column="isSync" type="com.aof.model.metadata.YesNo" />
		
		<property name="po_number" column="po_number" type="string" />
		<property name="po_line" column="po_line" type="string" /> 
		<property name="po_supp" column="po_supp" type="string" /> 
		<property name="po_date" column="po_date" type="date" />
		<property name="po_supp_name" column="po_supp_name" type="string" />
		
		<property name="in_date_line" column="in_date_line" type="date" />
		<property name="remark" column="remark" type="string" />
	</class>
	<!-- 调料单  -->
	<class name="PurchaseOrderCondimentSingle" table="po_condiment_single">
		<id name="id" column="id" type="integer">
			<generator class="native" />
		</id>
		<many-to-one name="po_detial_id" column="po_detial_id" class="PurchaseOrderInspectionPendingItem" />
		<many-to-one name="part" column="part" class="com.aof.model.basic.WmsPart" />
		<many-to-one name="supplier" column="supplier" class="com.aof.model.admin.Supplier" />
		
		<property name="number" column="number" type="big_decimal" />
		<property name="delivery_qty" column="delivery_qty" type="big_decimal" />
		<property name="putIn_qty" column="putIn_qty" type="big_decimal" />
		<property name="date" column="date" type="date" />
		<property name="supplier_part" column="supplier_part" type="string" />
		<property name="line" column="line" type="string" />
		<property name="status" column="status" type="com.aof.model.metadata.PurchaseOrderCondimentSingleStatus" />
		<property name="remark" column="remark" type="string" />
		<property name="code" column="code" type="string" />
		<property name="isPrint" column="isprint" type="integer"></property>
	</class>
	<!-- 质检信息  -->
	<class name="PurchaseOrderRqc" table="po_rqc">
		<id name="id" column="id" type="integer">
			<generator class="native" />
		</id>
		<many-to-one name="site" column="site" class="com.aof.model.admin.Site" />
		<many-to-one name="poipItem" column="poipItem" class="PurchaseOrderInspectionPendingItem" />
		<many-to-one name="single" column="single" class="PurchaseOrderCondimentSingle" />
		<many-to-one name="part" column="part" class="com.aof.model.basic.WmsPart" />
		<many-to-one name="supper" column="supper" class="com.aof.model.admin.Supplier" />
		<many-to-one name="boxId" column="box_id" class="Box" />
		
		<property name="status" column="status" type="com.aof.model.metadata.PurchaseOrderRqcStatus" />
		<property name="create_date" column="create_date" type="date" />
		<property name="qty" column="qty" type="big_decimal" />
		<property name="need_qty_rqc" column="need_qty_rqc" type="big_decimal" />
		<property name="actual_qty_rqc" column="actual_qty_rqc" type="big_decimal" />
		<property name="rqc_type" column="rqc_type" type="com.aof.model.metadata.YesNo" />
		<property name="qualified_qty" column="qualified_qty" type="big_decimal" />
		<property name="unqualified_qty" column="unqualified_qty" type="big_decimal" />
		<property name="remark" column="remark" type="string" />
		
		<property name="po_date" column="po_date" type="date" />
		<property name="po_qty" column="po_qty" type="big_decimal" />
		<property name="line" column="line" type="string" />
		<property name="po_number" column="po_number" type="string" />
		
		
	</class>
	<!-- 质检批次关联表  -->
	<class name="PurchaseOrderRqcBox" table="po_rqc_box">
		<id name="id" column="id" type="integer">
			<generator class="native" />
		</id>
		
		<many-to-one name="box_id" column="box_id" class="Box" />
		<many-to-one name="rqc_id" column="rqc_id" class="PurchaseOrderRqc" />
	</class>
	
	<class name="PurchaseOrderRqcUnqualified" table="po_rqc_unqualified">
		<id name="id" column="id" type="integer">
			<generator class="native" />
		</id>
		<many-to-one name="reasons" column="reasons" class="com.aof.model.basic.BadReasons" />
		<many-to-one name="rqc_box_id" column="rqc_box_id" class="Box" />
		
		<property name="qty" column="qty" type="big_decimal" />
		<property name="test1" column="test1" type="string" />
		<property name="test2" column="test2" type="string" />
		<property name="test3" column="test3" type="string" />
	</class>
	
	<!-- 采购入库  -->
	<class name="PurchaseOrderPutInStorage" table="po_putInStorage">
		<id name="id" column="id" type="integer">
			<generator class="native" />
		</id>
		<many-to-one name="poipItem" column="poipItem" class="PurchaseOrderInspectionPendingItem" />
		<many-to-one name="location" column="location" class="com.aof.model.basic.StorageLocation" />
		<many-to-one name="part" column="part" class="com.aof.model.basic.WmsPart" />
		<many-to-one name="supper" column="supper" class="com.aof.model.admin.Supplier" />
		<many-to-one  name="single" column="single"  class="PurchaseOrderCondimentSingle"/>
		
		<property name="date" column="date" type="date" />
		<property name="receipts_qty" column="receipts_qty" type="big_decimal" />
		<property name="qty" column="qty" type="big_decimal" />
		<property name="is_sync" column="is_sync" type="com.aof.model.metadata.YesNo" />
		<property name="status" column="status" type="com.aof.model.metadata.PurchaseOrderPutInStorageStatus" />
		
		<property name="po_date" column="po_date" type="date" />
		<property name="po_qty" column="po_qty" type="big_decimal" />
		<property name="line" column="line" type="string" />
		<property name="po_number" column="po_number" type="string" />
		<many-to-one name="lotSer" column="lot_ser" class="WmsLot" />
	</class>
	
	<class name="PurchaseOrderInspectionPending" table="purchaseOrderInspectionPending">
		<id name="poip_number" column="poip_number" type="string">
			<generator class="assigned" />
		</id>
		<many-to-one name="site" column="site" class="com.aof.model.admin.Site" />
		<many-to-one name="department" column="department" class="com.aof.model.admin.Department" />
		<many-to-one name="supplier" column="supplier_code" class="com.aof.model.admin.Supplier" />

		<property name="createDate" column="create_Date" type="timestamp" />
		<property name="po_number" column="po_number" type="string" />
		<property name="status" column="status" type="com.aof.model.metadata.PurchaseOrderStatus" />
		<property name="createType" column="createType" type="integer" />
		<property name="remark" column="remark" type="string" />
		<property name="describe" column="describe" type="string" />
		<property name="pdfmd5" column="pdfmd5" type="string" />
		<property name="isPrint" column="isPrint" type="com.aof.model.metadata.YesNo"/>
		<property name="contacter" column="contacter" type="string" />
		<property name="shipmentTo" column="shipmentTo" type="string" />
		<property name="ifConfirmPO" column="ifConfirmPO" type="com.aof.model.metadata.YesNo" />
	</class>

	<class name="PurchaseOrderInspectionPendingItem" table="purchaseOrderInspectionPending_item">
		<id name="id" column="id" type="integer">
			<generator class="native" />
		</id> 
		<many-to-one name="poip_number" column="poip_number" class="PurchaseOrderInspectionPending" />
		<many-to-one name="itemNumber" column="item_number" class="com.aof.model.basic.WmsPart" />
		
		<property name="line" column="line" type="string" />
		<property name="supplierItemNumber" column="supplier_item_number" type="string" />
		<property name="um" column="um" type="string" />
		<property name="rum" column="rum" type="string" />
		<property name="qty" column="qty" type="big_decimal" />
		<property name="dueDate" column="due_date" type="date" />
		<property name="boxCount" column="boxCount" type="integer" />

		<property name="qtyOpen" column="qty_open" type="big_decimal" />
