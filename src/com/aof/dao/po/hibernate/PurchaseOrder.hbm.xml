		<property name="capacity" column="capacity" type="big_decimal" />
		
		
		<property name="ad_attn" column="ad_attn" type="string" />
		<property name="po_ship" column="po_ship" type="string" />
		<property name="vd_promo" column="vd_promo" type="string" />
		<property name="po_confirm" column="po_confirm" type="integer" />
		<property name="po_domain" column="po_domain" type="string" />
		<property name="qty_std" column="qty_std" type="big_decimal" />
		<property name="isTxt" column="isTxt" type="com.aof.model.metadata.YesNo"/>
	</class>
	
	<!-- ==============================发货单开始2015-02-03-st================================================ -->
